{
  "name": "Free Kinopoisk",
  "version": "0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "js": ["index.js"],
      "css": ["styles.css"],
      "matches": ["https://www.kinopoisk.ru/*"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "/icons/icon-16x16.png",
      "32": "/icons/icon-32x32.png",
      "48": "/icons/icon-48x48.png"
    }
  },
  "icons": {
    "16": "/icons/icon-16x16.png",
    "32": "/icons/icon-32x32.png",
    "48": "/icons/icon-48x48.png"
  }
}
